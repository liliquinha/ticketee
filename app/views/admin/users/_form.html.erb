	<%= form_for [:admin, @user] do |f| %>
		<%= f.error_messages %>
		<p>
			<%= f.label :email, "Email" %>
			<%= f.text_field :email%>
		</p>
		<p>
			<%= f.label :password, "Password" %>
			<%= f.password_field :password %>
		</p>
		<p>
			<%= f.check_box :admin %>
			<%= f.label :admin, "Admin?" %>
		</p>
		<%= f.submit %>
	<% end %>